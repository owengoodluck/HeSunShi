@import '/assets/fonts/iconfont.wxss';
.printMthodHead {
  height: 78rpx;
  background-color: #f7f7f7;
  width: 100%;
  line-height: 78rpx;
}
.head-tab {
  color: #2b2b2b;
  width: 33%;
  text-align: center;
  font-size: 28rpx;
  margin-left: 20rpx;
}

.head-tab-all {
  color: #696969;
  width: 50%;
  text-align: right;
  font-size: 24rpx;
  margin-right: 20rpx;
}
.icon-red{
  color: red;
}
.icon-gray{
  color: gray;
}
.icon-white{
  color: white;
}
.pringMethodContent{
  width: 100%;
}
.content-title{
  text-align: center;
  font-weight: bold;
  font-size: 28rpx;
  margin-top: 20rpx;
}
.content-text{
  text-align: center;
  font-size: 24rpx;
  color: #9f9f9f;
  margin-top: 20rpx;
}
.content-img{
  width: 100%;
  align-items:center;
  display: flex;
  justify-content: center;
  text-align: center;
  margin-top: 20rpx;
}
.printMthodBtn{
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-top: 20rpx;
  padding-bottom: 100rpx;
}

.button {
  height: 98rpx;
  line-height: 98rpx;
  background: rgba(231, 0, 16, 1);
  border-radius: 30px;
  font-size: 15px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}
.line{
  border: 1px solid #ebe4e4;
}
.line-red{
  margin-top: 20rpx;
  border: 1px solid red;
}