.item {
  border-bottom: 1px solid rgba(151, 151, 151, 0.1);
  height: 116rpx;
}

.title {
  height: 116rpx;
  font-size: 16px;
  color: rgba(51,51,51,1);
  line-height: 116rpx;
}

.img {
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
}

.subButton {
  height: 100rpx;
  line-height: 100rpx;
  background: #e70010;
  border-radius: 25px;
  font-size: 16px;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}

button::after {
  border: none;
}

.row {
  height: 88rpx;
  display: flex;
  align-items: center; /*垂直*/
  justify-content: left; /*水平*/
  font-size: 14px;
  color: #030303;
  line-height: 40rpx;
}

.weui-uploader__bd {
  margin-bottom: -4px;
  /* margin-right: -9px; */
  overflow: hidden;
}

.weui-uploader__file {
  float: left;
  margin-right: 9px;
  margin-bottom: 9px;
}

.weui-uploader__input-box {
  float: left;
  position: relative;
  /* margin-left: 20rpx; */
  margin-right: 9px;
  margin-bottom: 9px;
  width: 77px;
  height: 77px;
  border: 1px solid #d9d9d9;
}

.weui-uploader__input-box:after, .weui-uploader__input-box:before {
  content: " ";
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}

.weui-uploader__input-box:before {
  width: 2px;
  height: 39.5px;
}

.weui-uploader__input-box:after {
  width: 39.5px;
  height: 2px;
}

.weui-uploader__input-box:active {
  border-color: #999;
}

.weui-uploader__input-box:active:after, .weui-uploader__input-box:active:before {
  background-color: #999;
}

.weui-uploader__input {
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}

.logoinfo {
  width: 220rpx;
  height: 220rpx;
  border-radius: 4px;
  position: relative;
}

.delete {
  width: 20px;
  height: 20px;
  border-radius: 60%;
  position: absolute;
  left: 200rpx;
}