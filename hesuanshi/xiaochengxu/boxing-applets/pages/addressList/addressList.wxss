page {
  background: #f5f5f5;
  padding-bottom: 120rpx;
}

.border {
  margin-top: 20rpx;
  background: #fff;
  padding: 0rpx 20rpx;
}

.addressDetail {
  padding: 28rpx 0rpx;
}

.name {
  float: left;
  font-size: 16px;
  font-weight: bold;
  color: #000;
}

.phone {
  float: right;
  font-size: 16px;
  color: #000;
}

.address {
  margin-top: 14rpx;
  font-size: 13px;
  color: #000;
}

.select {
  width: 30rpx;
  height: 30rpx;
}

.editIcon {
  width: 30rpx;
  height: 30rpx;
}

.delIcon {
  width: 30rpx;
  height: 30rpx;
  margin-left: 42rpx;
}

.setMoren {
  height: 76rpx;
  line-height: 76rpx;
  text-align: center;
}

.left {
  float: left;
  display: flex;
  align-items: center;
  justify-content: center;
}

.right {
  float: right;
  display: flex;
  align-items: center;
  justify-content: center;
}

.setMorenText {
  font-size: 13px;
  color: #999;
  margin-left: 10rpx;
}

.addAddressvView {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  border: 1px solid rgba(151, 151, 151, 0.1);
}

.addAddressIcon {
  width: 36rpx;
  height: 36rpx;
}

.addAddressText {
  color: #E70010;
  margin-left: 16rpx;
}

.subBtn {
  width: 100%;
  height: 102rpx;
  background: #fff;
  border-radius: 0;
  font-size: 16px;
  display: flex;
  align-items: center;
  justify-content: center;
}

button::after {
  border: none;
  border-radius: 0;
}
