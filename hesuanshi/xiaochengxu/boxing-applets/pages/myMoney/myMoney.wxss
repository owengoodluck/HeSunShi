page {
  background: #f9f9f9;
}

.title {
  height: 280rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  padding-top: 56rpx;
  background-image: url("data:image/png;base64,");
}

.title-text {
  font-size: 14px;
  color: #fff;
  margin-left: 20rpx;
}

.title-money {
  font-size: 58px;
  color: #fff;
  margin: 30rpx 0 0 20rpx;
}

.rowView {
  height: 114rpx;
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  background: #fff;
  border-bottom: 1rpx solid #979797;
}

.rowImg {
  width: 44rpx;
  height: 44rpx;
}

.rowText {
  font-size: 17px;
  color: rgba(51, 51, 51, 1);
  margin-left: 16rpx;
}
