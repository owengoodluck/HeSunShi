page {
  background: rgba(245, 245, 245, 1);
  padding-bottom: 30rpx;
}

.statusView {
  height: 112rpx;
  margin: 20rpx;
  display: flex;
  align-items: center;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 0rpx 8rpx 0rpx rgba(238, 238, 238, 1);
  border-radius: 8rpx;
}

.statusLeft {
  float: left;
  display: flex;
  align-items: center;
  width: 85%;
}

.head_img {
  width: 80rpx;
  height: 80rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 0rpx 4rpx 0rpx rgba(0, 0, 0, 0.1);
  border-radius: 8rpx;
  margin-left: 20rpx;
}

.head_status {
  height: 48rpx;
  font-size: 17px;
  color: rgba(3, 3, 3, 1);
  line-height: 48rpx;
  margin-left: 20rpx;
}

.statusRight {
  float: right;
  display: flex;
  align-items: center;
  text-align: center;
  flex-direction: column;
  width: 15%;
}

.head_kefu {
  width: 100rpx;
  height: 28rpx;
  font-size: 10px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 28rpx;
}

.kefuIcon {
  width: 40rpx;
  height: 40rpx;
}

.expressView {
  display: flex;
  align-items: center;
  margin: 0 20rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 0rpx 8rpx 0rpx rgba(238, 238, 238, 1);
  border-radius: 8rpx 8rpx 0 0;
  height: 138rpx;
}

.expressIcon {
  margin: 0 20rpx;
  width: 92rpx;
  height: 92rpx;
  border-radius: 46rpx;
}

.expressName {
  height: 40rpx;
  font-size: 14px;
  color: rgba(3, 3, 3, 1);
  line-height: 40rpx;
}

.expressPhone {
  height: 28rpx;
  font-size: 10px;
  color: rgba(3, 3, 3, 1);
  margin-top: 6rpx;
  line-height: 28rpx;
}

.expressNumber {
  display: flex;
  align-items: center;
  margin: 0 20rpx;
  height: 62rpx;
  background: rgba(250, 250, 250, 1);
  font-size: 12px;
  color: rgba(3, 3, 3, 1);
  line-height: 34rpx;
  padding-left: 20rpx;
  border-radius: 0 0 8rpx 8rpx;
}

.border {
  margin: 20rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 0rpx 8rpx 0rpx rgba(238, 238, 238, 1);
  border-radius: 8rpx;
  padding-bottom: 20rpx;
}

.logisit {
  position: relative;
  width: 100%;
  text-align: center;
}

.list {
  position: relative;
  display: inline-block;
  width: 85%;
  margin-top: 30rpx;
}

.daImg {
  display: inline-flex;
  position: absolute;
  left: -16rpx;
  top: 0rpx;
  width: 40rpx;
  height: 40rpx;
}

.zhongImg {
  display: inline-flex;
  position: absolute;
  left: -6rpx;
  top: 10rpx;
  width: 20rpx;
  height: 20rpx;
}

.xiaoImg {
  display: inline-flex;
  position: absolute;
  left: -2rpx;
  top: 16rpx;
  width: 12rpx;
  height: 12rpx;
}

.list_context_page {
  position: relative;
  width: 100%;
  top: 25%;
  left: 40rpx;
}

.time {
  position: relative;
  width: 100%;
  top: 30%;
  color: rgba(153, 153, 153, 1);
  text-align: left;
  font-size: 12px;
  left: 40rpx;
}

.context {
  position: relative;
  text-align: left;
  font-size: 14px;
  width: 550rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  color: rgba(153, 153, 153, 1);
}

.contextBold {
  color: rgba(51, 51, 51, 1);
  font-weight: bold;
}
