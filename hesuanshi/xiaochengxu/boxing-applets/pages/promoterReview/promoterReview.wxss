page {
  padding-bottom: 50rpx;
  background: #f5f5f5;
}

.title {
  width: 100%;
  height: 560rpx;
}

.text {
  display: flex;
  align-items: center; /*垂直*/
  justify-content: center;
  height: 28px;
  font-size: 20px;
  color: rgba(3, 3, 3, 1);
  line-height: 28px;
}

.bottom-btn {
  position: fixed;
  bottom: 0;
  height: 98rpx;
  display: flex;
}

.btn {
  height: 98rpx;
  line-height: 98rpx;
  border-radius: 0px;
  font-size: 15px;
  color: rgba(255, 255, 255, 1);
}

button::after {
  border: none;
}

.home {
  width: 136rpx;
  height: 136rpx;
  position: fixed;
  bottom: 150rpx;
  right: 20rpx;
}
