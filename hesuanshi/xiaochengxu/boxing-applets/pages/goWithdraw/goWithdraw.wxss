page {
  background: #f9f9f9;
  padding-bottom: 50rpx;
}

.titleView {
  border-top: 2rpx solid rgba(0, 0, 0, 0.14);
  background: #fff;
  height: 414rpx;
  text-align: center;
}

.title-img {
  margin-top: 52rpx;
  width: 270rpx;
  height: 130rpx;
}

.title-text {
  margin-top: 28rpx;
  height: 20px;
  font-size: 14px;
  color: #333;
  line-height: 20px;
}

.title-money {
  height: 56px;
  font-size: 40px;
  color: rgba(51, 51, 51, 1);
  line-height: 56px;
}

.tixianView {
  background: #fff;
  padding: 0 20rpx;
}

.tixianView_left {
  float: left;
  display: flex;
  align-items: center;
  height: 94rpx;
  line-height: 94rpx;
}

.tixianView_text {
  font-size: 14px;
  color: rgba(51, 51, 51, 1);
}

.tixianView_right {
  float: right;
  display: flex;
  align-items: center;
  height: 94rpx;
  line-height: 94rpx;
}

.tixianView_right_icon {
  width: 12rpx;
  height: 26rpx;
  margin-left: 32rpx;
}

.result {
  margin: 128rpx 20rpx 0 20rpx;
  padding-bottom: 100rpx;
}

.button {
  height: 98rpx;
  line-height: 98rpx;
  background: rgba(231, 0, 16, 1);
  border-radius: 30px;
  font-size: 15px;
  color: rgba(255, 255, 255, 1);
}

.button::after {
  border: 0;
}
