<view>
  <view class='titleView'>
    <image class='title-img' src='/images/usa-money.png'></image>
    <view class='title-text'>包含待结算{{pendingCommission}}元</view>
    <view class='title-money'>{{canCommission}}</view>
  </view>

  <view class='tixianView' style='margin-top: 20rpx;' bindtap='goIncomeAndExpensesTap'>
    <view class='tixianView_left'>
      <text class='tixianView_text'>佣金记录</text>
    </view>
    <view class='tixianView_right'>
      <image src='/images/left.png' class='tixianView_right_icon'></image>
    </view>
    <view style='clear:both;'></view>
  </view>
  <view style='width: 100%;height: 2rpx;background: #FFF;'>
    <view style='width: 710rpx;height: 2rpx;background: #E9E8E8;margin:0 20rpx;'></view>
  </view>
  <view class='tixianView' bindtap='goWithdrawRecordTap'>
    <view class='tixianView_left'>
      <text class='tixianView_text'>提现记录</text>
    </view>
    <view class='tixianView_right'>
      <image src='/images/left.png' class='tixianView_right_icon'></image>
    </view>
    <view style='clear:both;'></view>
  </view>
  <view class='result'>
    <button class='button' bindtap='tixianTap'>提现</button>
  </view>
</view>